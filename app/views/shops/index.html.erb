<%= link_to "Flavors", "/flavors" %>
<%= link_to "Shops", "/shops" %>

<h1>All Shops:</h1>

<% @shops.each do |shop| %>
  <h3 style="display:inline"><a href="/shops/<%= shop.id %>"><%= shop.name%></a></h3>
  <small><%= "Created at: #{shop.created_at}" %></small>  <%= link_to "Edit", "/shops/#{shop.id}/edit" %>
  <small><%= link_to "Delete Shop", "/shops/#{shop.id}", method: :delete %></small>
</br>
<% end %></br>

<h3><%= link_to "New Shop", "/shops/new" %></h3>
